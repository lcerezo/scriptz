#turns off isdn for desktop systems
#turns off isdn and disables it at start.
# Jan 25, 2006 | Initial Creation | Luis E. Cerezo


control:
	actionsequence = ( processes shellcommands )


		shellcommands:
			
			AllLinuxWorkstations|memdhcp.linux::	
				
				"/sbin/chkconfig --list isdn | /bin/grep :on "	useshell=true
										define=isdn_chkconfig_off
				
			isdn_chkconfig_off::
	
				"/sbin/chkconfig isdn off"	useshell=false
				
