                                                                        server=${catchall_server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
                                                                        type=sum
									backup=false	
                                                                        recurse=inf
                                                                        define=HUPautofs

	HOU.!GandE.!depth::
