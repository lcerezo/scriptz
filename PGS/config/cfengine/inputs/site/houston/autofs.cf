	control:
	
		actionsequence = ( copy shellcommands )
		AddInstallable = ( failure )
		
	copy:

##
##copy all the maps over to new dir
##shouldnt be a need to key this on certain classes anymore
	HOU::
                ${configroot}/config/cfengine/inputs/site/houston/autofs       dest=/etc/autofs_pgs/
                                                                        server=${server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
                                                                        type=sum
                                                                        recurse=inf
									ignore=CVS
									backup=false	
                                                                        define=HUPautofs
                                                                        failover=failure 
	HOU.failure::
                ${configroot}/config/cfengine/inputs/site/houston/autofs       dest=/etc/autofs_pgs/
                                                                        server=${catchall_server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
                                                                        type=sum
									backup=false	
									ignore=CVS
                                                                        recurse=inf
                                                                        define=HUPautofs
###auto direct maps fir HYPERBEAM machines

        HOU.hyperbeam::
                ${configroot}/config/cfengine/inputs/site/houston/auto.direct.hyperBeam        dest=/etc/autofs_pgs/auto.direct
                                                                        server=${server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
                                                                        type=sum
                                                                        recurse=inf
									backup=false	
                                                                        define=HUPautofs
                                                                        failover=failure
        HOU.hyperbeam.failure::
                ${configroot}/config/cfengine/inputs/site/houston/auto.direct.hyperBeam        dest=/etc/autofs_pgs/auto.direct
                                                                        server=${catchall_server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
                                                                        type=sum
                                                                        recurse=inf
									backup=false	
                                                                        define=HUPautofs
###auto direct maps for the /- for PT

        HOU.GandE::
                ${configroot}/config/cfengine/inputs/site/houston/auto.direct.pt        dest=/etc/autofs_pgs/auto.direct
                                                                        server=${server}
                                                                        owner=root
                                                                        group=root
                                                                        mode=0644
