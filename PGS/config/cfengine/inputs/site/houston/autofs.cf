                                                                        define=HUPautofs
									failover=failure
									backup=false	
        (HOU.!GandE.!depth).failure::

                ${configroot}/config/cfengine/inputs/site/houston/auto.master     dest=/etc/auto.master
                                                                        server=${catchall_server}
                                                                        owner=root
